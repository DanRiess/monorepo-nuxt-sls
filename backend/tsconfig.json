{
	"compilerOptions": {
		"target": "ES2022",
		"lib": ["ES2023"],
		"module": "Node16",
		"moduleResolution": "Node16",
		"skipLibCheck": true,
		"esModuleInterop": true,
		"declaration": false,
		"outDir": "dist",
		"strict": true,
		"strictNullChecks": true
	},
	"include": ["./tsup.config.ts", "src"],
	"exclude": ["node_modules", "**/*.spec.ts"]
}
